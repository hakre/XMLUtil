{
    "name": "hakre/xmlutil",
    "description": " Utility Classes for XML Standard Applications in PHP",
    "keywords": ["XML"],
    "homepage": "https://github.com/hakre/XMLUtil",
    "minimum-stability": "stable",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "hakre",
            "homepage": "http://hakre.wordpress.com/"
        }
    ],
    "suggest": {
        "ext-iconv": "*"
      , "ext-hash": "*"
    },
    "autoload": {
        "classmap": ["src/"]
    },
